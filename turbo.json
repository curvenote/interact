{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build#demo-fourier": {
      "dependsOn": ["common", "thebe-redux"],
      "outputs": ["build/**"]
    },
    "build#demo-remix": {
      "dependsOn": ["common", "thebe-redux"]
    },
    "build#demo-web": {
      "dependsOn": ["common", "thebe-redux"]
    },
    "start": {
      "outputs": ["build/**"]
    },
    "lint": {
      "outputs": []
    }
  }
}
